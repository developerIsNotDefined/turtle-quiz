{
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "env": {
    "node": true
  },
  "rules": {
    "require-jsdoc": 0,
    "linebreak-style": "off",
    "comma-dangle": "off",
    "arrow-parens": "off",
    "max-len": ["warn", 100],
    "eol-last": 0,
    "one-var": "off",
    "guard-for-in": "warn",
    "curly": ["error", "multi-or-nest"]
  }
}